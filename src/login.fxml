<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="34.0" layoutY="-1.0" prefHeight="276.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-1.0">
            <children>
                <JFXTextField fx:id="uID" focusColor="DARKORANGE" labelFloat="true" prefHeight="56.0" prefWidth="380.0" promptText="User ID">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXPasswordField fx:id="password" focusColor="DARKORANGE" labelFloat="true" prefHeight="56.0" prefWidth="400.0" promptText="Password">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="30.0" />
                    </VBox.margin>
                </JFXPasswordField>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <JFXButton fx:id="enterButton" buttonType="RAISED" mnemonicParsing="false" onAction="#makeLogin" prefHeight="115.0" prefWidth="199.0" style="-fx-background-color: #ff8c00;" text="Enter" />
                        <JFXButton fx:id="cancelButton" buttonType="RAISED" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancel" prefHeight="115.0" prefWidth="199.0" style="-fx-background-color: #ff8c00;" text="Cancel" textAlignment="CENTER" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets top="20.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>